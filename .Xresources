! Colors
URxvt*highlightColor:           #9a875f
URxvt*highlightTextColor:       #917154
URxvt*cursorColor:              #9c7650
URxvt*pointerColor:             #515e6f
URxvt*colorBD:                  #9c7650
URxvt*colorUL:                  #585858
URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secundaryScroll: True
URxvt*saveLines : 8000
URxvt*cursorBlink: true
URxvt*fading:   0
URxvt*shading:  0
URxvt*letterSpace: -1
 
urxvt*background:       #0c0d0e
urxvt*foreground:       #7f8f9f

! black + red
urxvt*color0:      #101010
urxvt*color1:      #705050
! green + yellow
urxvt*color2:      #60b48a
urxvt*color3:      #f0dfaf
! blue + purple
urxvt*color4:      #506070
urxvt*color5:      #dc8cc3
! cyan + white
urxvt*color6:      #8cd0d3
urxvt*color7:      #dcdccc
! bright-black + bright-red
urxvt*color8:      #000000
urxvt*color9:      #dca3a3
! bright-green + bright-yellow
urxvt*color10:     #c3bf9f
urxvt*color11:     #f0dfaf
! bright-blue + bright-purple
urxvt*color12:     #94bff3
urxvt*color13:     #ec93d3
! bright-cyan + bright-white
urxvt*color14:     #93e0e3
urxvt*color15:     #ffffff


Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

! fonts
! run "fc-list" for a list of available fonts
URxvt*font:xft:Monospace:pixelsize=11,xft:Inconsolata\ for\ Powerline:pixelsize=11

URxvt.perl-ext-common : default,matcher,tabbed
URxvt.urlLauncher     : qutebrowser        
URxvt.matcher.button  : 1
